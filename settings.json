{
  "github.copilot.advanced": {
    "language": "french",
    "inlineSuggestCount": 3,
    "listCount": 5,
    "debug.showScores": true,
    "debug.overrideEngine": "davinci",
    "snippetLength": "medium",
    "enableAutoCompletions": true,
    "enableGhostText": true,
    "indentationMode": "automatic",
    "pasteSupport": true,
    "suggestMode": "inline",
    "contextWindow": 20,
    "temperature": 0.4,
    "topP": 0.95,
    "delayCompletions": false,
    "enableTabComplete": true,
    "features": {
      "copilotIgnore": true,
      "copilotWorkspace": true,
      "semanticHighlighting": true,
      "inlineCompletion": true,
      "documentationGeneration": true
    },
    "patterns": {
      "typescript": {
        "suggest": {
          "snippets": true,
          "imports": true,
          "types": true
        }
      },
      "angular": {
        "suggest": {
          "components": true,
          "templates": true,
          "services": true,
          "testing": true
        }
      }
    }
  }
}
